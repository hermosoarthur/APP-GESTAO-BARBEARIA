      "adaptiveIcon": {
        "foregroundImage": "./assets/logoicon.jpeg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/logoicon.jpeg"
    },
    "extra": {
      "eas": {
        "projectId": "be7e7fd2-3666-4bfb-a1b2-e9cd30bc1119"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
